    <% if user_signed_in? %>
    <%= form_with url: search_path, method: :get, local: :true do |f| %>
      <%= f.select :range, options_for_select([["User"], ["Book"]]) %>
      <%= f.select :search, options_for_select([["完全一致","perfect_match"],["前方一致","front_match"], ["後方一致","back_match"],["部分一致","partial_match"]]) %>
      <%= f.text_field :word %>
      <%= f.submit :search %>
    <% end %>
    <% end %>
